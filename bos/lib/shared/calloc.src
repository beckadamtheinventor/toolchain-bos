        assume  adl=1

	public _calloc
_calloc:
	pop	de
	pop	bc
	pop	hl
	push	hl
	push	bc
	push	de
	call	__imulu
	push	hl
	call	_sys_Malloc
	add	hl,de
	xor	a,a
	sbc	hl,de
	ld	e,a
	push	de
	push	hl
	call	nz,_sys_MemSet
	pop	hl
	pop	de
	pop	bc
	ret

	extern	__imulu
	extern	_sys_Malloc
	extern	_sys_MemSet
